{
  "expo": {
    "name": "PlanMoney",
    "slug": "PlanMoney",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "planmoney",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.pallav1503.PlanMoney",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#012744"
      },
      "permissions": [
        "NOTIFICATIONS"
      ],
      "googleServicesFile": "./google-services.json",
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.ico"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageResizeMode": "contain",
          "backgroundColor": "#012744",
          "imageWidth":200
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/ic_notification.png",
          "color": "#ffff",
          "enableVibration": true,
          "enableLights": true,
          "enableBackgroundRemoteNotifications": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9b287878-2e94-496e-951f-a84bda272f44"
      }
    }
  }
}